{
    "name": "python-venv",
    "version": "0.0.1",
    "description": "create and use python virtual environments",
    "main": "./lib/venv.js",
    "scripts": {
        "test": "mocha",
        "postinstall": "node lib/postInstall.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rrbutani/node-python-venv.git"
    },
    "keywords": [
        "venv",
        "virtualenv"
    ],
    "author": "Rahul Butani",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/rrbutani/node-python-venv/issues"
    },
    "homepage": "https://github.com/rrbutani/node-python-venv#readme",
    "devDependencies": {
        "mocha": "^3.2.0"
    },
    "dependencies": {
        "child_process": "^1.0.2"
    },
    "python-venv": {
        "pythonExecName": "python3",
        "pythonExecFallback": false,
        "pipExecName": "pip3",
        "venvVersion": "15.0.0",
        "venvFlags": [
            "--system-site-packages"
        ]
    }
}
